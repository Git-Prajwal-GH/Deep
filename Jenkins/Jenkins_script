pipeline {
    agent any
    tools{
        jdk 'java-11'
        maven 'maven'
    }
    stages{
        stage('git checkout'){
            steps{
                git branch: 'main', url: 'https://github.com/Git-Prajwal-GH/Deep.git'
            }
        }
        stage('compile'){
            steps{
                sh "mvn compile"
            }
        }

        }
    }
